    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>9dcb2ad5-c755-4b1d-8fd7-34f830fa53f3</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="7.0.8">
